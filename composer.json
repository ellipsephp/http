{
    "name": "ellipse/http",
    "type": "library",
    "description": "Psr-15 based http kernel",
    "keywords": [
        "psr-15",
        "psr-7",
        "middleware",
        "request-handler"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/http",
    "support": {
        "issues": "https://github.com/ellipsephp/http/issues",
        "source": "https://github.com/ellipsephp/http"
